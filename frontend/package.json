{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-dev": "tsc && vite build --mode development",
    "docker:build": "docker build -t adonisv79/akosi-app:${npm_package_version} .",
    "docker:build-win": "docker build -t adonisv79/akosi-app:%npm_package_version% .",
    "docker:publish": "docker push adonisv79/akosi-app:${npm_package_version}",
    "docker:publish-win": "docker push adonisv79/akosi-app:%npm_package_version%",
    "generate:types": "pnpx openapi-typescript http://localhost:3001/api-json -o src/api/api.types.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.18.1",
    "axios": "^1.6.7",
    "i18next": "^23.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/blocks": "^7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/test": "^7.6.12",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "openapi-typescript": "7.0.0-next.7",
    "postcss": "^8.4.33",
    "storybook": "^7.6.12",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-i18n": "^1.0.2"
  }
}
